<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
  
<configuration>
	<typeAliases>
		<typeAlias alias="MyConcert" type="concert.model.ConcertVo"/>
		<typeAlias alias="MyBoard" type="board.model.BoardBean"/>
		<typeAlias type="board.model.BoardReplyBean" alias="MyBoardReply" />
		<typeAlias alias="MyUser" type="user.model.UserVo"/>
	</typeAliases>
	<mappers>
		<mapper resource="mybatis/concert.xml"/>
		<mapper resource="mybatis/board.xml"/>
		<mapper resource="mybatis/boardreply.xml"/>
		<mapper resource="mybatis/admin.xml"/>
		<mapper resource="mybatis/club.xml"/>
		<mapper resource="mybatis/user.xml"/>
		
		<mapper resource="admin/mybatis/board.xml"/>
		<mapper resource="admin/mybatis/admin.xml"/>
		<mapper resource="admin/mybatis/club.xml"/>
		<mapper resource="admin/mybatis/user.xml"/>
	</mappers>
	
</configuration>